# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=1000

# SendGrid Configuration
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=noreply@bidsense.ca
SENDGRID_FROM_NAME=BidSense

# Railway Configuration
RAILWAY_TOKEN=your_railway_token
RAILWAY_PROJECT_ID=your_railway_project_id

# Apify Configuration (for proxy pool)
APIFY_API_TOKEN=your_apify_api_token

# PostHog Analytics
POSTHOG_API_KEY=your_posthog_api_key
POSTHOG_HOST=https://app.posthog.com

# Application Configuration
ENVIRONMENT=development
DEBUG=true
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
API_BASE_URL=http://localhost:8000

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/bidsense

# Email Configuration
EMAIL_DIGEST_FREQUENCY=daily
EMAIL_DIGEST_TIME=07:00

# Scraping Configuration
SCRAPER_INTERVAL_HOURS=1
SCRAPER_TIMEOUT_SECONDS=30
SCRAPER_MAX_RETRIES=3

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Feature Flags
ENABLE_AI_SUMMARIES=true
ENABLE_VECTOR_SEARCH=true
ENABLE_EMAIL_DIGEST=true
ALPHA_MODE=true 